import sys

def get_first():
	try:
		return int(sys.argv[1]) 
	except Exception:
		print("Первое число должно состоять из цифр!")
def get_second():
	try:
		return int(sys.argv[2]) 
	except Exception:
		print("Второе число должно состоять из цифр!")

if len (sys.argv) > 1:
	first_num = get_first()
	second_num = get_second()
	print("Сумма: "+str(first_num+second_num))
else:
	print("Аргументов нет")
  
